<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <remote  name="colt"
           fetch="https://github.com/Colt-Enigma"
           revision="refs/heads/c12" />

  <remote  name="colt-devices"
           fetch="https://github.com/ColtOS-Devices"
           revision="refs/heads/c12" />

<project path="manifest" name="platform_manifest" remote="colt" />
  <project path="build/make" name="platform_build_make" remote="colt" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/blueprint" name="platform_build_blueprint" groups="pdk,tradefed" remote="colt" />
  <project path="build/soong" name="platform_build_soong" groups="pdk,tradefed" remote="colt" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <project path="art" name="platform_art" remote="colt" />
  <project path="bionic" name="platform_bionic" groups="pdk" remote="colt" />
  <project path="bootable/recovery" name="platform_bootable_recovery" remote="colt" />

  <project path="libcore" name="platform_libcore" groups="pdk" remote="colt" />

  <!-- external repos -->
  <project path="external/json-c" name="platform_external_json-c" remote="colt" />
  <project path="external/selinux" name="platform_external_selinux" remote="colt" />
  <project path="external/tinycompress" name="platform_external_tinycompress" remote="colt" />
  <project path="external/tinyxml" name="platform_external_tinyxml" remote="colt" />
  <project path="external/colorkt" name="platform_external_colorkt" remote="colt" />
  <project path="external/themelib" name="platform_external_themelib" remote="colt" />

  <!-- framework repos -->
  <project path="frameworks/av" name="platform_frameworks_av" remote="colt" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="colt" />
  <project path="frameworks/libs/systemui" name="platform_frameworks_libs_systemui" groups="pdk-cw-fs,pdk-fs" remote="colt" />
  <project path="frameworks/native" name="platform_frameworks_native" remote="colt" />

  <!-- packages repos -->
  <project path="packages/apps/Camera2" name="platform_packages_apps_Camera2" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/Contacts" name="platform_packages_apps_Contacts" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/Dialer" name="platform_packages_apps_Dialer" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/DeskClock" name="platform_packages_apps_DeskClock" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/DocumentsUI" name="platform_packages_apps_DocumentsUI" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/Messaging" name="platform_packages_apps_Messaging" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/ThemePicker" name="platform_packages_apps_ThemePicker" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/WallpaperPicker2" name="platform_packages_apps_WallpaperPicker2" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/Launcher3" name="platform_packages_apps_Launcher3" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/SimpleDeviceConfig" name="platform_packages_apps_SimpleDeviceConfig" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/SettingsIntelligence" name="platform_packages_apps_SettingsIntelligence" groups="pdk-fs" remote="colt" />
  <project path="packages/apps/Settings" name="platform_packages_apps_Settings" remote="colt" />

  <!-- packages providers repos -->
  <project path="packages/providers/DownloadProvider" name="platform_packages_providers_DownloadProvider" groups="pdk-fs" remote="colt" />
  <project path="packages/providers/MediaProvider" name="platform_packages_providers_MediaProvider" groups="pdk-fs" remote="colt" />
  <project path="packages/providers/TelephonyProvider" name="platform_packages_providers_TelephonyProvider" groups="pdk-fs" remote="colt" />

  <!-- packages Modules repos -->
  <project path="packages/modules/Permission" name="platform_packages_modules_Permission" groups="pdk-fs" remote="colt" />
  <project path="packages/modules/Wifi" name="platform_packages_modules_Wifi" groups="pdk-fs" remote="colt" />
  <project path="packages/modules/Connectivity" name="platform_packages_modules_Connectivity" groups="pdk-fs" remote="colt" />

  <!-- prebuilt repos -->
  <project path="prebuilts/abi-dumps/vndk" name="platform_prebuilts_abi-dumps_vndk" clone-depth="1" remote="colt" />

  <!-- system repos -->
  <project path="system/core" name="platform_system_core" remote="colt" />
  <project path="system/tools/mkbootimg" name="platform_system_tools_mkbootimg" groups="pdk" remote="colt" />
  <project path="system/update_engine" name="platform_system_update_engine" groups="pdk" remote="colt" />

  <!-- telphony repos -->
  <project path="packages/services/Telecomm" name="platform_packages_services_Telecomm" groups="pdk-fs" remote="colt" />

  <!-- vendor repos -->
  <project path="vendor/colt" name="platform_vendor_colt" remote="colt" />
  <project path="vendor/codeaurora/telephony" name="platform_vendor_codeaurora_telephony" remote="colt" />
  <project path="vendor/qcom/opensource/power" name="platform_vendor_qcom_opensource_power" remote="colt" />
  <project path="vendor/qcom/opensource/vibrator" name="platform_vendor_qcom_opensource_vibrator" remote="colt" />

</manifest>
